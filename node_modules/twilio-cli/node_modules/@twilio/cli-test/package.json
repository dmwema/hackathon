{
  "name": "@twilio/cli-test",
  "description": "Test scaffolding for the twilio-cli",
  "version": "2.1.2",
  "author": "Twilio @twilio",
  "bugs": "https://github.com/twilio/twilio-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.7.0",
