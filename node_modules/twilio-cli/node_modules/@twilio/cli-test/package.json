    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "nock": "^13.0.2",
    "sinon": "^9.0.2",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "eslint": "^7.4.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-plugin-mocha": "^7.0.1",
